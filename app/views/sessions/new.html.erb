<div id="flash_dumbass">
	<%= flash[:dumbass] %>
</div>

<div id="flash_alert">
	<%= flash[:alert] %>
</div>


<div id="errors">
	<%= flash[:errors] %>
	<h2> Login to My Jams!</h2>

	<%= form_for(:session, url:login_path) do |f| %>
		<%= f.label :username, 'username' %>
		<%= f.text_field :username %>

		<%= f.label :password, 'password' %>
		<%= f.password_field :password %>

	<% end %>
</div>
